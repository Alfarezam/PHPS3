;\r\n                $barang->hargaBarang = $hargaBarang;\r\n                $barang->jumlahBarang = $jumlahBarang;\r\n                $this->saveToSession();\r\n                return true;\r\n            }\r\n        }\r\n        return false;\r\n    }\r\n\r\n    public function deleteBarang($id) {\r\n        foreach ($this->barangs as $key => $barang) {\r\n            if ($barang->idBarang == $id){\r\n                unset($this->barangs[$key]);\r\n                $this->saveToSession();\r\n                return true;\r\n            }\r\n        }\r\n        return false;\r\n    }\r\n}\r\n?>
